{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "name": "Booking"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "date": {
      "type": "string"
    },
    "start": {
      "type": "time"
    },
    "end": {
      "type": "time"
    },
    "mealTray": {
      "type": "boolean"
    },
    "laptop": {
      "type": "boolean"
    },
    "room": {
      "via": "bookings",
      "model": "room"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "bookings"
    }
  }
}
